{
  "name": "vue3-mediaelement",
  "version": "0.0.5",
  "author": "Rajendra Uppu",
  "license": "MIT",
  "description": "video live player",
  "github": "https://github.com/RajendraUppu/vue3-mediaelement#readme",
  "bugs": {
    "url": "https://github.com/RajendraUppu/vue-mediaelement/issues"
  },
  "main": "./dist/vue3-mediaelement.umd.ts",
  "module": "./dist/vue3-mediaelement.es.ts",
  "keywords": [
    "MP4",
    "hls",
    "video",
    "m3u8"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue3-mediaelement.es.ts",
      "require": "./dist/vue3-mediaelement.umd.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite ",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "mediaelement": "^5.1.0",
    "vue": "^3.2.38"
  },
  "devDependencies": {
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7"
  }
}
